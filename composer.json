{
  "name": "lti/dotsub-api",
  "type": "library",
  "description": "Client library for the DotSUB API",
  "keywords": [
    "dotsub",
    "linguistic team international"
  ],
  "homepage": "http://dev.linguisticteam.org",
  "authors": [
    {
      "name": "Bruno De Carvalho",
      "email": "decarvalho.bruno@free.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "satooshi/php-coveralls": "dev-master"
  },
  "support": {
    "email": "linguisticteam@gmail.com"
  },
  "suggest": {
    "google/apiclient": "This package's structure is drawn from the Google PHP API Client"
  },
  "autoload": {
    "psr-4": {
      "Lti\\DotsubAPI\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer dump-autoload --optimize"
    ]
  }
}
